    "ConnectionStrings": {
        "MainDbContext": "Server={CONNECTION_STRING_HOST};Initial Catalog={CONNECTION_STRING_DATABASE};Persist Security Info=False;User ID={CONNECTION_STRING_USER};Password={CONNECTION_STRING_PASSWORD};"
    },
    "AllowedHosts": "*"
}
